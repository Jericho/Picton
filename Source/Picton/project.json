{
	"title": "Picton",
	"version": "1.0.0-*",
	"description": "The Picton library for Azure",
	"authors": [ "jericho" ],
	"packOptions": {
		"owners": [ "jericho" ],
		"summary": "Among other things it contains extension methods and abstrations for StorageAccount, BlobClient, QueueClient, etc.",
		"projectUrl": "https://github.com/Jericho/Picton",
		"licenseUrl": "http://jericho.mit-license.org",
		"iconUrl": "https://github.com/identicons/jericho.png",
		"requireLicenseAcceptance": false,
		"tags": [ "Picton", "Azure" ],
		"repository": {
			"type": "git",
			"url": "https://github.com/Jericho/Picton"
		}
	},

	"dependencies": {
		"WindowsAzure.Storage": "8.1.1",
		"Wire": "0.8.1"
	},
	"frameworks": {
		"net452": {
			"dependencies": {
				"System.Diagnostics.Tools": "4.0.0"
			},
			"buildOptions": {
				"define": [ "NETFULL" ]
			}
		},
		"netstandard1.6": {
			"imports": "dnxcore50",
			"dependencies": {
				"NETStandard.Library": "1.6.0",
				"System.Diagnostics.Process": "4.1.0",
				"System.Diagnostics.Tools": "4.0.1"
			},
			"buildOptions": {
				"define": [ "NETSTANDARD" ]
			}
		}
	}
}

{
	"buildOptions": {
		"emitEntryPoint": false,
		"xmlDoc": false,
		"platform": "anycpu",
		"additionalArguments": [
			"/ruleset:../Picton.ruleset",
			"/additionalfile:../stylecop.json"
		],
		"compile": {
		}
	},
	"configurations": {
		"Release": {
			"buildOptions": {
				"warningsAsErrors": true
			}
		}
	},
	"dependencies": {
		"StyleCop.Analyzers": {
			"version": "1.1.0-beta001",
			"type": "build"
		},
		"Wire": "0.8.2"
	},
	"frameworks": {
		"net452": {
			"dependencies": {
				"System.Diagnostics.Tools": "4.0.0",
				"System.Net.Http": "4.0.0",
				"System.Net.Requests": "4.0.0",
				"WindowsAzure.Storage": "8.0.0"
			},
			"buildOptions": {
				"define": [ "NETFULL" ]
			}
		},
		"netstandard1.6": {
			"imports": "dnxcore50",
			"dependencies": {
				"NETStandard.Library": "1.6.1",
				"System.Collections.Specialized": "4.3.0",
				"System.Diagnostics.Process": "4.3.0",
				"System.Diagnostics.Tools": "4.3.0",
				"System.Net.Http": "4.3.1",
				"System.Net.Requests": "4.3.0",
				"WindowsAzure.Storage": "8.1.1"
			},
			"buildOptions": {
				"define": [ "NETSTANDARD" ]
			}
		}
	},
	"version": "1.5.0-drop-old-frameworks.1"
}

{
	"version": "1.0.0-*",
	"testRunner": "xunit",

	"buildOptions": {
		"platform": "anycpu"
	},

	"dependencies": {
		"Picton": { "target": "project" },
		"xunit": "2.2.0-beta4-build3444",
		"dotnet-test-xunit": "2.2.0-preview2-build1029",
		"Moq": "4.6.38-alpha",
		"Shouldly": "2.8.2"
	},

	"frameworks": {
		"netcoreapp1.0": {
			"buildOptions": {
				"define": [ "NETCORE" ]
			},
			"imports": [
				"dnxcore50",
				"portable-net451+win8"
			],
			"dependencies": {
				"Microsoft.NETCore.App": {
					"type": "platform",
					"version": "1.0.1"
				},
				"System.Runtime.Serialization.Primitives": "4.3.0",
				"System.Runtime.Serialization.Formatters": "4.3.0"
			}
		},
		"net452": {
			"buildOptions": {
				"define": [ "NETFULL" ]
			},
			"dependencies": {
			},
			"frameworkAssemblies": {
				"System.Runtime": {
					"type": "build"
				},
				"System.Runtime.Serialization": "4.0.0"
			}
		}
	}
}
